# Google OAuth Configuration
# Get these credentials from Google Cloud Console: https://console.cloud.google.com/
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# NextAuth Configuration
# The NEXTAUTH_SECRET is used to encrypt the NextAuth.js JWT
# You can generate a secure secret using: openssl rand -base64 32
NEXTAUTH_SECRET=your_nextauth_secret_here

# NextAuth URL Configuration
# Set this to your application's base URL
NEXTAUTH_URL=http://localhost:3000

# Database connection
DATABASE_URL=your_database_url_here

# OpenAI
OPENAI_API_KEY=your_openai_here
